        </Affector>
    </AnimationDefinition>
    
    <AnimationDefinition autoStart="false" duration="0.8" name="RefreshPortMouseOverAnimation" replayMode="loop">
        <Affector applicationMethod="relative" interpolator="QuaternionSlerp" property="Rotation">
            <KeyFrame position="0.0" progression="linear" value="x:0 y:0 z:0" />
            <KeyFrame position="0.4" progression="linear" value="x:0 y:0 z:180" />
            <KeyFrame position="0.8" progression="linear" value="x:0 y:0 z:360" />
        </Affector>
    </AnimationDefinition>
    <AnimationDefinition autoStart="false" duration="0.2" name="RefreshPortMouseClickAnimation" replayMode="once">
        <Affector applicationMethod="absolute" interpolator="float" property="Alpha">
            <KeyFrame position="0.0" sourceProperty="Alpha" />
            <KeyFrame position="0.19" progression="quadratic decelerating" value="0.6" />
            <KeyFrame position="0.2" progression="discrete" value="1.0" />
        </Affector>
    </AnimationDefinition>
    <AnimationDefinition autoStart="false" duration="0.5" name="ButtonMouseOverAnimation" replayMode="loop">
